import javax.swing.JOptionPane;
import processing.sound.*;
SoundFile file;



void setup()
{
  size(500, 300);
  background(#364E6C);

  JOptionPane.showMessageDialog(null, "Megaman's White Christmas!",
    "Merry Christmas to all",
    JOptionPane.PLAIN_MESSAGE);


  JOptionPane.showMessageDialog(null, "And a Happy New Year",
    "Wishing you a Merry Christmas",
    JOptionPane.PLAIN_MESSAGE);


  //Array asking how many cookies should Santa get
  int nums[]=new int[1];
  int sum=0;

  for (int i=0; i<nums.length; i++)
  {
    nums[i] = Integer.parseInt(JOptionPane.showInputDialog("How many Cookies should be left out for Santa?", "6"));
    sum = sum + nums[i];
  }
  println("Cookies left for Santa " + sum);

  // Load a soundfile from the /data folder of the sketch and play it back
  file = new SoundFile(this, "7.mp3");
  file.play();

  println("Left Mouse button, Mega man blinks");
  println("Right Mouse button, Megaman aims buster cannon");
  println("Press Key, Megaman shoots snowballs");
  println("Mouse Drag left or right, Megaman changes form(color)");

  String quote = "Spreading the Christmas cheer, Megaman in a santa hat!";
  String sub = quote.substring(0, 54);
  println(sub);

  //My own method String
  String incorrectquote= "I am not santa claus";
  println(incorrectquote.toUpperCase());
}
